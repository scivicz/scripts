local lib = loadstring(game:HttpGet("https://pastebin.com/raw/9m51VUEE", true))()

function notify(title,text,duration,closebutton)
	game.StarterGui:SetCore("SendNotification", 
		{
			Title = title;
			Text = text;
			Duration = duration;
			Button1 = closebutton
		}
	)
end

lib.SetName("Prison Life Gui")
lib:CreateButton("Give Remington",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP
	}

	workspace.Remote.ItemHandler:InvokeServer(unpack(args))

end)
lib:CreateButton("Give M9",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = workspace.Prison_ITEMS.giver.M9.ITEMPICKUP
	}

	workspace.Remote.ItemHandler:InvokeServer(unpack(args))

end)

lib:CreateButton("Give AK-47",function()
	local args = {
		[1] = workspace.Prison_ITEMS.giver.M9.ITEMPICKUP
	}

	workspace.Remote.ItemHandler:InvokeServer(unpack(args))
end)

lib:CreateButton("Give Keycard",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = workspace.Prison_ITEMS.single:FindFirstChild("Key card").ITEMPICKUP
	}

	workspace.Remote.ItemHandler:InvokeServer(unpack(args))

end)

lib:CreateButton("Give Hammer",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP
	}

	workspace.Remote.ItemHandler:InvokeServer(unpack(args))

end)

lib:CreateButton("Join Guards",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = "Bright blue"
	}

	workspace.Remote.TeamEvent:FireServer(unpack(args))

end)

lib:CreateButton("Join Immates",function()
	-- Script generated by SimpleSpy - credits to exx#9394

	local args = {
		[1] = "Bright orange"
	}

	workspace.Remote.TeamEvent:FireServer(unpack(args))

end)
















lib:CreateButton("Copy Invite",function()
	setclipboard("https://discord.gg/QbYceNx2Zq")
	notify("Notification","Copied invite to clipboard",15,"Okay")
end)

lib:CreateButton("Credits",function()
	notify("Notification","Created by tryfulsss#0710 and Packwatch#0001",15,"Close")
end)

lib:CreateButton("Destroy Gui",function()
	game.CoreGui.A:Destroy()
end)
